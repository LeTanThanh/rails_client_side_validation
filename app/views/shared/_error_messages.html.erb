<div class="panel panel-danger">
  <div class="panel-heading">
    Form conrains <%= pluralize object.errors.full_messages.length, "error"  %>
  </div>

  <div class="panel-body">
    <ul>
      <% object.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
</div>
