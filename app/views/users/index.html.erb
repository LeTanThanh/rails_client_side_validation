<div class="container">
  <h2>Users</h2>
        
  <table class="table">
    <thead>
      <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <%= render "users/user", user:user %>
      <% end %>
    </tbody>
  </table>
</div>
